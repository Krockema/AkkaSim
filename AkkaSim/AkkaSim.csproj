<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageProjectUrl>https://github.com/Krockema/AkkaSim</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>DES Simulation implementation, with pesimistic central clock</Description>
    <Copyright>MIT</Copyright>
    <Version>0.3.2</Version>
    <PackageReleaseNotes>Debuging Enabled.</PackageReleaseNotes>
    <License>https://github.com/Krockema/AkkaSim/blob/master/LICENSE</License>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Akka" Version="1.3.16" />
  </ItemGroup>

</Project>
